# Domo API Configuration
# Get your credentials from: https://developer.domo.com/

# Domo Client ID (from your Domo OAuth client)
DOMO_CLIENT_ID=your_client_id_here

# Domo Client Secret (from your Domo OAuth client)
DOMO_CLIENT_SECRET=your_client_secret_here

# Domo Dataset ID (default is the retirement fee dataset)
DOMO_DATASET_ID=your_dataset_id_here

# Set to 'true' to use Domo API, 'false' to use local CSV file
NEXT_PUBLIC_USE_DOMO_API=false
